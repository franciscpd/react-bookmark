{
  "name": "react-bookmarks",
  "version": "0.0.1",
  "description": "Bookmarks for user",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --watch --coverage --config=./configs/jest/jest.json",
    "start": "NODE_ENV=development webpack-dev-server --mode development --inline --progress --config=./configs/webpack/dev.js",
